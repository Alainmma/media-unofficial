[Unit]
Description=MediaTomb multimedia server
Requires=network.target
After=network.target

[Service]
Type=simple
ExecStart=/usr/bin/mediatomb -c /etc/mediatomb/config.xml -u mediatomb -g mediatomb -P /var/run/mediatomb.pid -l /var/log/mediatomb.log -m /var/lib -f mediatomb

[Install]
WantedBy=multi-user.target
